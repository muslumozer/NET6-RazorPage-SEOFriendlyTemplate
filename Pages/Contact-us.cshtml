@page
@model NET6RazorPageSEOFriendlyTemplate.Pages.Contact_usModel
@{
    ViewData["Title"] = "Contact Us - NET6RazorPageSEOFriendlyTemplate";
    ViewData["Canonical"] = "https://www.localhost.com/contact-us/";
}

<h1 class="text-center">Contact Us</h1>
<br />
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <p>You can reach us from the form below, at <a href="mailto:admin@localhost.com">admin@localhost.com</a> or on the map.</p>
            <p>Phone Number : <a href="tel:+15555551234">+1 (555) 555-1234</a></p>
            <br />
        </div>

        <div class="col-md-4">
            <div class="mb-3">
                <label for="exampleFormControlInput1" class="form-label">Email address</label>
                <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
            </div>
            <div class="mb-3">
                <label for="exampleFormControlTextarea1" class="form-label">Messages</label>
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
            </div>
            <button type="button" class="btn btn-primary" value="Send Email" onclick="window.open('mailto:admin@localhost.com');">Send</button>
            <br />
            <br />
        </div>

        <div class="offset-md-1 col-md-6 offset-md-1">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3306.7713662923375!2d-118.49939638448039!3d34.0240792266606!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80c2a4c80526f219%3A0xdc1b2a1940f53f94!2s1030%20Lincoln%20Blvd%2C%20Santa%20Monica%2C%20CA%2090403%2C%20Amerika%20Birle%C5%9Fik%20Devletleri!5e0!3m2!1str!2str!4v1646950825852!5m2!1str!2str" width="100%" height="400" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
            <div />
        </div>
    </div>
</div>
<br />
<br />